  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "lint": "nx workspace-lint && nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2020 browser module main",
    "clean": "rimraf tmp coverage dist",
    "build:all": "nx run-many --target build --all --",
    "build:nestjs-utils": "nx run nestjs-utils:build --",
    "build:nestjs-config": "nx run nestjs-config:build --",
    "build:nestjs-logger": "nx run nestjs-logger:build --",
    "build:nestjs-sqlite3": "nx run nestjs-sqlite3:build --",
    "build:hot-server": "nx run hot-server:build --",
    "build:hot-gateway": "nx run hot-gateway:build --",
    "build:hot-cli": "nx run hot-cli:build --",
    "pretest:all": "rimraf coverage",
    "posttest:all": "./build/merge-coverage.ts",
    "posttest:nestjs-utils": "./build/merge-coverage.ts",
    "posttest:nestjs-config": "./build/merge-coverage.ts",
    "posttest:nestjs-logger": "./build/merge-coverage.ts",
    "posttest:nestjs-sqlite3": "./build/merge-coverage.ts",
    "posttest:hot-server": "./build/merge-coverage.ts",
    "test": "npm run test:all",
    "test:all": "nx run-many --target=test --all --parallel --codeCoverage --coverageReporters=lcov,json,text-summary",
    "test:nestjs-utils": "nx run nestjs-utils:test --parallel --codeCoverage --coverageReporters=lcov,json,text-summary",
    "test:nestjs-config": "nx run nestjs-config:test --parallel --codeCoverage --coverageReporters=lcov,json,text-summary",
    "test:nestjs-logger": "nx run nestjs-logger:test --parallel --codeCoverage --coverageReporters=lcov,json,text-summary",
    "test:nestjs-sqlite3": "nx run nestjs-sqlite3:test --parallel --codeCoverage --coverageReporters=lcov,json,text-summary",
    "test:hot-server": "nx run hot-server:test --parallel --codeCoverage --coverageReporters=lcov,json,text-summary",
    "lint:all": "nx workspace-lint && nx run-many --target=lint --all",
    "lint:nestjs-utils": "nx run nestjs-utils:lint",
    "lint:nestjs-config": "nx run nestjs-config:lint",
    "lint:nestjs-logger": "nx run nestjs-logger:lint",
    "lint:nestjs-sqlite3": "nx run nestjs-sqlite3:lint",
    "lint:hot-server": "nx run hot-server:lint",
    "ci": "npm run test:all && npm run lint:all && npm run build:all",
    "ci:nestjs-utils": "npm run test:nestjs-utils && npm run lint:nestjs-utils && npm run build:nestjs-utils",
    "ci:nestjs-config": "npm run test:nestjs-config && npm run lint:nestjs-config && npm run build:nestjs-config",
    "ci:nestjs-logger": "npm run test:nestjs-logger && npm run lint:nestjs-logger && npm run build:nestjs-logger",
    "ci:nestjs-sqlite3": "npm run test:nestjs-sqlite3 && npm run lint:nestjs-sqlite3 && npm run build:nestjs-sqlite3",
    "ci:hot-server": "npm run test:hot-server && npm run lint:hot-server && npm run build:hot-server",
    "preci": "npm run clean",
    "postci": "build/prepare-publish.ts",
    "publish:nestjs-utils": "cd dist/projects/node/libs/nestjs-utils && npm publish --access public",
    "publish:nestjs-config": "cd dist/projects/node/libs/nestjs-config && npm publish --access public",
    "publish:nestjs-logger": "cd dist/projects/node/libs/nestjs-logger && npm publish --access public",
    "publish:nestjs-sqlite3": "cd dist/projects/node/libs/nestjs-sqlite3 && npm publish --access public",
    "hot-cli": "ts-node -P projects/node/apps/hot-gateway/tsconfig.cli.json projects/node/apps/hot-gateway/src/main.ts --"
  },
